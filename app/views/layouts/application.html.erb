<!DOCTYPE html>
<html>
  <head>
    <title>CommunityBoard</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'partials/nav' %>
    <% if flash[:notice] %>
      <div><%= flash[:notice] %></div>
    <% end %>
    <%= yield %>
  </body>
</html>
